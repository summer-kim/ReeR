  },
  "homepage": "https://github.com/summer-kim/ReeR#readme",
  "dependencies": {
    "aws-sdk": "^2.820.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-sass": "^5.0.0",
    "request": "^2.88.2",
