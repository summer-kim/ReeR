{
  "name": "reer",
  "version": "1.0.0",
  "description": "Movie Series Review platform",
  "main": "server.js",
  "scripts": {
    "sass": "node-sass -w scss/ -o dist/css/ --recursive",
    "server": "nodemon --exec ts-node www.ts --watch",
    "start": "node --loader ts-node/esm ./www.ts",
    "dev": "concurrently \"npm run server\" \"npm run start --prefix client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/summer-kim/ReeR.git"
  },
  "author": "Summer Kim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/summer-kim/ReeR/issues"
  },
  "homepage": "https://github.com/summer-kim/ReeR#readme",
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.2",
    "aws-sdk": "^2.820.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "node-sass": "^5.0.0",
    "request": "^2.88.2",
    "sequelize": "^6.6.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2",
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
